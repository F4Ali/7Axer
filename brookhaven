wait(10.5)
game.StarterGui:SetCore("SendNotification", {
    Title = "مطور السكربت علي";
    Text = "اتمنى يعجبك سكربت حقي 😊"; -- BY WBA_ZEYAD --
    Duration = 5;
})

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/jensonhirst/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "مطور السكربت علي", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

-- كود السحب للجوال والماوس
task.wait(1)

local CoreGui = game:GetService("CoreGui")
local gui = CoreGui:FindFirstChild("Orion") or game.Players.LocalPlayer:WaitForChild("PlayerGui"):FindFirstChild("Orion")

if gui then
    local mainFrame = gui:FindFirstChildOfClass("Frame")
    if mainFrame then
        local dragging = false
        local dragInput, dragStart, startPos

        local function update(input)
            local delta = input.Position - dragStart
            mainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
                                           startPos.Y.Scale, startPos.Y.Offset + delta.Y)
        end

        mainFrame.InputBegan:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.Touch or input.UserInputType == Enum.UserInputType.MouseButton1 then
                dragging = true
                dragStart = input.Position
                startPos = mainFrame.Position

                input.Changed:Connect(function()
                    if input.UserInputState == Enum.UserInputState.End then
                        dragging = false
                    end
                end)
            end
        end)

        game:GetService("UserInputService").InputChanged:Connect(function(input)
            if dragging and (input.UserInputType == Enum.UserInputType.Touch or input.UserInputType == Enum.UserInputType.MouseMovement) then
                update(input)
            end
        end)
    end
end

local Tab = Window:MakeTab({
	Name = "BROOKHAVEN سكربتات لماب",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "سكربتات تخريب😈💀"
})

